#ifndef _EXT_INT_TYPES_H_
#define _EXT_INT_TYPES_H_

typedef enum
{
	EXT_INT0,
	EXT_INT1,
	EXT_INT2

}ext_int_type;

typedef enum
{
	LOW_LEVEL,
	BOTH_EDGES,
	FALLING_EDGE,
	RISING_EDGE
}sense_mode;

